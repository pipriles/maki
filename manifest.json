{
  "manifest_version": 3,
  "name": "Scaper",
  "version": "0.1",
  "permissions": [
    "tabs",
    "downloads"
  ],
  "action": {},
  "content_security_policy": {
    "extension_pages": "script-src 'self'; object-src 'self'"
  },
  "content_scripts": [{
    "matches": [
      "<all_urls>"
    ],
    "js": [
      "content.js"
    ]
  }],
  "background": {
    "service_worker": "background.js"
  }
}
